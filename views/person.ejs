<html>
    <head>
        <link href="/assets/style.css" type="text/css" rel="stylesheet" />
    </head>
    <body>
        <h1>Person: <%= ID %></h1>
    <% if (ID%2 == 0) { %>
        <% for (let i=0; i<(ID*2); i++) { %>
            <h1>ES PAR</h1>
        <% }} else { %>
            <% for (let i=0; i<(ID); i++) { %>
                <h1>ES IMPAR</h1>
    <% }} %>
    </body>
</html>